{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowJs": true,
    "target": "esnext",
    "moduleResolution": "node",
    "typeRoots": ["./types", "./node_modules/@types"],
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "src",
    "types",
    "images.d.ts"
  ]
}
